<?xml version='1.0' encoding='utf-8'?>
<widget id="com.thomaskaldahl.skyline" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Skyline</name>
    <description>
		Your insightful academic planner and scheduler.
	</description>
    <author email="thomas@thomaskaldahl.com" href="https://thomaskaldahl.com">
		Thomas Kaldahl
	</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
